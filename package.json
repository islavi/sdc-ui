{
  "name": "sdc-ui",
  "version": "1.1.3",
  "description": "This project aims to create a unified UI styled components for multiple development teams who work on the same web-based applications.",
  "main": "lib/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-typescript": "tsc -p tsconfig.json",
    "build-scss": "node-sass --include-path scss scss/style.scss css/style.css",
    "build-react": "babel src/react -d lib/react && babel src/index.js -o lib/index.js",
    "build": "npm run build-typescript && npm run build-react && npm run build-scss && node build-demo.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .out"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onap-sdc/sdc-ui.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/onap-sdc/sdc-ui/issues"
  },
  "files": [
    "lib",
    "css"
  ],
  "homepage": "https://github.com/onap-sdc/sdc-ui#readme",
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-cli": "^6.24.1",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "copy": "^0.3.0",
    "css-loader": "^0.28.0",
    "html-loader": "^0.4.5",
    "node-sass": "^4.5.2",
    "prismjs": "^1.6.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "typescript": "~2.0.10",
    "@types/node": "^6.0.48"
  },
  "dependencies": {
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "@angular/common": "~2.2.1",
    "@angular/compiler": "~2.2.1",
    "@angular/core": "~2.2.1",
    "rxjs": "5.0.0-beta.12"
  }
}
